{
  "name": "bento-box",
  "version": "0.4.2",
  "description": "Application framework for Node.js",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/dstreet/bento-box.git"
  },
  "scripts": {
    "test": "./node_modules/mocha/bin/mocha test/unit/**/*",
    "coverage": "./node_modules/mocha/bin/mocha test/unit --require blanket -R html-cov > coverage.html"
  },
  "author": "David Street",
  "license": "MIT",
  "devDependencies": {
    "blanket": "^1.1.7",
    "chai": "^2.3.0",
    "chai-spies": "^0.6.0",
    "mocha": "^2.2.4"
  },
  "dependencies": {
    "extend": "^2.0.1",
    "hashids": "^1.0.1"
  },
  "config": {
    "blanket": {
      "pattern": "",
      "data-cover-never": [
        "node_modules",
        "test"
      ],
      "data-cover-reporter-options": {
        "shortnames": true
      }
    }
  }
}
